<application>
  <component name="ActionMacroManager">
    <macro name="close_panes">
      <action id="EditorEscape" />
      <action id="ActivateProjectToolWindow" />
    </macro>
    <macro name="Open File From Clipboard">
      <action id="GotoFile" />
      <action id="EditorPaste" />
    </macro>
    <macro name="Open File At Word">
      <action id="EditorSelectWord" />
      <action id="GotoFile" />
    </macro>
    <macro name="split line">
      <action id="GotoAction" />
      <typing text-keycode="83:0;80:0;76:0;73:0;84:0">split</typing>
      <action id="SplitAction" />
      <action id="EditorCopy" />
      <action id="EditorPaste" />
    </macro>
    <macro name="aa">
      <action id="FindInPath" />
    </macro>
  </component>
</application>
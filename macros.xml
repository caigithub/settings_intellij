<application>
  <component name="ActionMacroManager">
    <macro name="close_panes">
      <action id="EditorEscape" />
      <action id="ActivateProjectToolWindow" />
    </macro>
    <macro name="Open File From Clipboard">
      <action id="GotoFile" />
      <action id="EditorPaste" />
    </macro>
    <macro name="Open File At Word">
      <action id="EditorSelectWord" />
      <action id="GotoFile" />
    </macro>
    <macro name="Open POM">
      <action id="GotoFile" />
      <typing text-keycode="32:0;80:0;79:0;77:0;46:0;88:0;77:0;76:0">&amp;#x20;pom.xml</typing>
      <action id="EditorLineStart" />
      <action id="EditorLineStart" />
    </macro>
  </component>
</application>